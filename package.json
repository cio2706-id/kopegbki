{
  "name": "koperasi-pegawai-webapp",
  "version": "1.0.0",
  "description": "Fullstack Koperasi Pegawai Biro Klasifikasi Indonesia Web Application",
  "main": "index.ts",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "ts-node index.ts",
    "client": "cd client && npm start",
    "build": "tsc && cd client && npm run build",
    "start": "node dist/index.js",
    "install-all": "npm install && cd client && npm install",
    "dev:backend": "nodemon --exec ts-node index.ts",
    "dev:frontend": "cd client && npm start"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "crypto": "^1.0.1",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.7",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "koperasi",
    "pegawai",
    "accurate",
    "loan",
    "cooperative"
  ],
  "author": "Koperasi Pegawai Biro Klasifikasi Indonesia",
  "license": "MIT"
}